{
    "name": "mautic/open-source-edition",
    "license": "MIT",
    "type": "project",
    "description": "Mautic Open Source Distribution",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.3.7",
        "symfony/symfony": "2.4.*",
        "doctrine/orm": "~2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~2.3",
        "sensio/framework-extra-bundle": "~3.0",
        "sensio/generator-bundle": "~2.3",

        "symfony/proxy-manager-bridge": "~2.5",
        "symfony/event-dispatcher": "2.5.*@dev",
        "symfony/finder": "2.3.*@dev",
        "doctrine/doctrine-fixtures-bundle": "2.2.*@dev",
        "doctrine/migrations": "dev-master",
        "doctrine/doctrine-migrations-bundle": "dev-master",
        "ocramius/proxy-manager": "0.5.*",
        "ircmaxell/password-compat": "~1.0",
        "knplabs/knp-menu": "2.0.*@dev",
        "knplabs/knp-menu-bundle":"dev-master",
        "friendsofsymfony/oauth-server-bundle": "dev-master",
        "jms/serializer-bundle": "@dev",
        "friendsofsymfony/rest-bundle": "1.3.*",

        "mpdf/mpdf": "v5.7.2",
        "phpoffice/phpexcel": "1.8.0"
    },
    "require-dev": {
        "liip/functional-test-bundle": "dev-master",
        "nelmio/api-doc-bundle": "2.5.*@dev",
        "webfactory/exceptions-bundle": "@stable"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
         ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "../"
    }
}
