{
  "env": {
    "browser": true
  },
  "extends": ["airbnb/base","prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "ignorePatterns": ["Assets/library/js/dist","node_modules","*.min.js"],
  "globals": {
    "mQuery": true,
    // "grapesjs": true,
    "Mousetrap":true,
    "Mautic":true,
    "mauticAjaxCsrf":true
  }
}
