{% set data = event.extra.stat.sentDetails %}

{% if data.failed is defined or data.timeline is not defined %}
    {# nothing to show #}
{% else %}
    <dl class="dl-horizontal">
        <dt>{{ 'mautic.dynamicContent.timeline.content'|trans }}</dt>
        <dd>{{ data.timeline|trans }}</dd>
    </dl>
{% endif %}